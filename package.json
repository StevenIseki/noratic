{
  "name": "noratic",
  "version": "1.0.4",
  "description": "a static site generator using node and react",
  "preferGlobal": true,
  "bin": {
    "noratic": "index.js"
  },
  "scripts": {
    "test": "tape test/*.js",
    "example": "webpack --config ./site/webpack.config.js",
    "serve": "node ./site/server.js",
    "start": "npm run example && npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/StevenIseki/noratic.git"
  },
  "keywords": [
    "static",
    "site",
    "generator"
  ],
  "author": "Steven Iseki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/StevenIseki/noratic/issues"
  },
  "homepage": "https://github.com/StevenIseki/noratic",
  "devDependencies": {
    "babel-core": "^5.8.3",
    "babel-loader": "^5.3.2",
    "react": "^0.13.3",
    "react-router": "^0.13.3",
    "webpack": "^1.10.5",
    "express": "^4.7.2"
  },
  "dependencies": {
    "ncp": "^2.0.0",
    "react-markdown-to-html": "^1.0.11"
  }
}
